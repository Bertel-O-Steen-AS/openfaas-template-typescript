{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "declaration": true,
    "rootDir": "./function",
    "outDir": "./function",
    "strict": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ]
}
