{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": ["es2019", "es2017", "es7", "es6", "dom", "esnext.asynciterable"],
    "declaration": true,
    "rootDir": "./function",
    "outDir": "./function",
    "strict": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "skipLibCheck": true,
    "types": ["node", "jest"]
  },
  "exclude": ["node_modules"]
}
